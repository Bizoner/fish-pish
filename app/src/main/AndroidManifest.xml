<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gadyez.fishpish">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fishpishlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fishpishlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" />
        <activity android:name=".SettingsActivity" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="862995c8eac52d690eda80e60775b13ed128695e" />

        <activity android:name=".LeaderBoardActivity"></activity>
    </application>

</manifest>